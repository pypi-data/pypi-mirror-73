{
    "__metadata__": {
        "service_key": "match",
        "service_product": "lol",
        "service_endpoint": "match",
        "service_version": "v4"
    },
    "requests": {
        "get_match": {
            "method": "GET",
            "url": "matches/${match_id}"
        },
        "get_matches": {
            "method": "GET",
            "url": "matchlists/by-account/${account_id}"
        },
        "get_timeline": {
            "method": "GET",
            "url": "timelines/by-match/${match_id}"
        },
        "get_tournament_match": {
            "method": "GET",
            "url": "matches/${match_id}/by-tournament-code/${tournament_code}"
        },
        "get_tournament_matches": {
            "method": "GET",
            "url": "matches/by-tournament-code/${tournament_code}/ids"
        }
    }
}