<?xml version="1.0"?>
<tryton>
    <data>

        <!-- report: note as odt -->
        <record model="ir.action.report" id="report_noteodt">
            <field name="name">Note (odt)</field>
            <field name="model">pim_memos.note</field>
            <field name="report_name">pim_memos.reportodt</field>
            <field name="report">pim_memos/report/note.fodt</field>
        </record>
        <record model="ir.action.keyword" id="report_noteodt_keyword">
            <field name="keyword">form_print</field>
            <field name="model">pim_memos.note,-1</field>
            <field name="action" ref="report_noteodt"/>
        </record>

    </data>
</tryton>
