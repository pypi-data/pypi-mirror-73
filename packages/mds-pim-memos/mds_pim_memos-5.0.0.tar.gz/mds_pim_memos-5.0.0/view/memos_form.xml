<form col="6">
    <label name="title"/>
    <field name="title" colspan="3"/>
    <label name="sequence"/>
    <field name="sequence"/>
    <label name="category"/>
    <field name="category"/>
    <label name="datecreated"/>
    <field name="datecreated" widget="date"/>
    <label name="datechanged"/>
    <field name="datechanged" widget="date"/>
