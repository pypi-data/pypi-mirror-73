        <!-- ## Validation test case: Intentionally different to layer to ensure there is a single error ## -->
        <westBoundLongitude>-1.0974053892215567</westBoundLongitude>
        <!-- ## Validation test case: Intentionally different to layer to ensure there is a single error ## -->
        <eastBoundLongitude>-1.0972053892215567</eastBoundLongitude>
        <southBoundLatitude>0.1436125748502993</southBoundLatitude>
        <northBoundLatitude>0.1438125748502993</northBoundLatitude>
      </EX_GeographicBoundingBox>
      <BoundingBox CRS="CRS:84" minx="-1.0974053892215567" miny="0.1436125748502993" maxx="-1.0972053892215567" maxy="0.1438125748502993"/>
      <BoundingBox CRS="EPSG:3031" minx="-237453.71096783454" miny="1.239595204840458E7" maxx="-237409.617608712" maxy="1.2395995857779771E7"/>
      <BoundingBox CRS="EPSG:4326" minx="0.1436125748502993" miny="-1.0974053892215567" maxx="0.1438125748502993" maxy="-1.0972053892215567"/>
      <Layer queryable="1" opaque="0">
        <Name>sample:sample-layer</Name>
        <Title>sample-layer</Title>
        <Abstract/>
        <KeywordList>
          <Keyword>features</Keyword>
          <Keyword>sample-layer</Keyword>
        </KeywordList>
        <CRS>EPSG:4326</CRS>
        <CRS>CRS:84</CRS>
        <EX_GeographicBoundingBox>
          <!-- ## Validation test case: Intentionally invalid longitude (less than -180) ## -->
          <westBoundLongitude>-190</westBoundLongitude>
          <eastBoundLongitude>-1.0972053892215567</eastBoundLongitude>
          <!-- ## Validation test case: Intentionally invalid longitude (less than -180) ## -->
          <southBoundLatitude>-190</southBoundLatitude>
          <northBoundLatitude>0.1438125748502993</northBoundLatitude>
        </EX_GeographicBoundingBox>
        <BoundingBox CRS="CRS:84" minx="-1.0974053892215567" miny="0.1436125748502993" maxx="-1.0972053892215567" maxy="0.1438125748502993"/>
        <BoundingBox CRS="EPSG:4326" minx="0.1436125748502993" miny="-1.0974053892215567" maxx="0.1438125748502993" maxy="-1.0972053892215567"/>
        <BoundingBox CRS="EPSG:3031" minx="-237453.71096783454" miny="1.239595204840458E7" maxx="-237409.617608712" maxy="1.2395995857779771E7"/>
        <Style>
          <Name>sample-style</Name>
          <Title>sample-style</Title>
          <LegendURL width="20" height="20">
            <Format>image/png</Format>
            <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://localhost:8080/geoserver/ows?service=WMS&amp;request=GetLegendGraphic&amp;format=image%2Fpng&amp;width=20&amp;height=20&amp;layer=sample%3Asample-layer"/>
          </LegendURL>
        </Style>
      </Layer>
    </Layer>
  </Capability>
</WMS_Capabilities>
