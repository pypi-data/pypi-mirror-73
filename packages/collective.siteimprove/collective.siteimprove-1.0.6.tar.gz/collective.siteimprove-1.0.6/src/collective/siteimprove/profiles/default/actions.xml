<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
  <object name="object_buttons">
    <object name="siteimprove" meta_type="CMF Action" i18n:domain="collective.siteimprove">
      <property name="title" i18n:translate="">Recheck with Siteimprove</property>
      <property name="description" i18n:translate=""></property>
      <property
          name="url_expr">string:#</property>
      <property name="available_expr">object/@@si-anon-can-view</property>
      <property name="permissions">
        <element value="collective.siteimprove: Use Siteimprove"/>
      </property>
      <property name="visible">True</property>
    </object>
  </object>
</object>
